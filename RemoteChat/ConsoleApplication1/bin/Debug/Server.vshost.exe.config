<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.runtime.remoting>
    <application name="RemoteServ">
      <service>
        <wellknown type="RemoteChat.RemoteObject, 
                             RemoteObj"
                   mode="Singleton"
                   objectUri="remoteObj"/>
      </service>
      <channels>
        <channel ref="tcp" port="6791">
          <serverProviders>
            <provider ref="binary" typeFilterLevel="Full" />
          </serverProviders>
        </channel>
      </channels>
    </application>
  </system.runtime.remoting>
</configuration>
